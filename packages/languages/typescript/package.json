{
  "name": "@justice/identity-protection-blockchain",
  "version": "1.0.0",
  "description": "Decentralized blockchain for identity protection and media tracking with Take It Down Act compliance",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./core": "./dist/core/index.js",
    "./web3": "./dist/web3/index.js",
    "./ai": "./dist/ai/index.js",
    "./compliance": "./dist/compliance/index.js",
    "./browser": "./dist/browser/index.js"
  },
  "scripts": {
    "build": "tsc && npm run build:contracts",
    "build:contracts": "hardhat compile",
    "build:clean": "rimraf dist",
    "prebuild": "npm run build:clean",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "ts-node src/index.ts",
    "start:node": "node dist/node.js",
    "deploy": "hardhat deploy --network mainnet",
    "prepublishOnly": "npm run build && npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "blockchain",
    "identity-protection", 
    "web3",
    "deepfakes",
    "privacy",
    "take-it-down",
    "decentralized",
    "ai",
    "cybersecurity"
  ],
  "author": "Justice Project",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.8.0",
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "ipfs-http-client": "^60.0.0",
    "libp2p": "^0.46.0",
    "web3": "^4.0.0",
    "crypto-js": "^4.1.1",
    "tweetnacl": "^1.0.3",
    "node-forge": "^1.3.1",
    "axios": "^1.5.0",
    "ws": "^8.14.0",
    "@noble/hashes": "^1.3.0",
    "@noble/curves": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/ws": "^8.5.5",
    "typescript": "^5.2.0",
    "hardhat": "^2.17.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "jest": "^29.6.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
